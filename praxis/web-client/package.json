{
  "name": "web-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "../../scripts/dev-frontend.sh",
    "start:browser": "ng serve --configuration=browser",
    "start:gh-pages": "ng serve --configuration=gh-pages",
    "jupyterlite:build": "cd ../.. && uv run jupyter lite build --config praxis/web-client/jupyterlite-config.json --output-dir praxis/web-client/src/assets/jupyterlite",
    "build:all": "npm run jupyterlite:build && ng build",
    "build": "ng build",
    "build:browser": "ng build --configuration=browser",
    "build:gh-pages": "ng build --configuration=gh-pages",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-api": "npx openapi-typescript-codegen --input ./src/assets/api/openapi.json --output ./src/app/core/api-generated --client fetch --useUnionTypes",
    "prebuild": "npm run generate-api"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/aria": "^21.0.5",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.5",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.0",
    "@ngrx/signals": "^21.0.0-rc.0",
    "@ngx-formly/core": "^7.0.1",
    "@ngx-formly/material": "^7.0.1",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/sql.js": "^1.4.9",
    "angular-plotly.js": "^20.0.0",
    "keycloak-angular": "^20.0.0",
    "keycloak-js": "^26.2.2",
    "marked": "^17.0.1",
    "mermaid": "^11.12.2",
    "ngx-markdown": "^21.0.1",
    "ngx-skeleton-loader": "^11.3.0",
    "plotly.js-dist-min": "^3.3.1",
    "pyodide": "^0.29.0",
    "rxjs": "~7.8.0",
    "shepherd.js": "^14.5.1",
    "sql.js": "^1.13.0",
    "tslib": "^2.3.0",
    "web-serial-polyfill": "^1.0.15",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.1.0",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/schematics": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "jsdom": "^27.1.0",
    "openapi-typescript-codegen": "^0.30.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.8"
  }
}
@@ -0,0 +1,169 @@
+
+import { Component, ElementRef, ViewChild, AfterViewInit, signal } from '@angular/core';
+import { CommonModule } from '@angular/common';
+import { Chart, registerables } from 'chart.js';
+import { MatSelectModule } from '@angular/material/select';
+import { MatFormFieldModule } from '@angular/material/form-field';
+import { MatButtonModule } from '@angular/material/button';
+import { MatIconModule } from '@angular/material/icon';
+import { FormsModule } from '@angular/forms';
+
+Chart.register(...registerables);
+
+interface DataPoint {
+  x: number;
+  y: number;
+  [key: string]: any;
+}
+
+@Component({
+  selector: 'app-data-visualization',
+  standalone: true,
+  imports: [
+    CommonModule,
+    MatSelectModule,
+    MatFormFieldModule,
+    MatButtonModule,
+    MatIconModule,
+    FormsModule
+  ],
+  template: `
+    <div class="p-6">
+      <div class="flex justify-between items-center mb-4">
+        <h1 class="text-3xl font-bold">Data Visualization</h1>
+        <button mat-flat-button color="primary" (click)="exportChart()" [disabled]="!data().length">
+          <mat-icon>save</mat-icon> Export
+        </button>
+      </div>
+
+      <div class="flex gap-4 mb-4">
+        <mat-form-field>
+          <mat-label>X-Axis</mat-label>
+          <mat-select [(ngModel)]="xAxis" (ngModelChange)="createChart()">
+            @for(axis of availableAxes; track axis) {
+              <mat-option [value]="axis">{{ axis }}</mat-option>
+            }
+          </mat-select>
+        </mat-form-field>
+        <mat-form-field>
+          <mat-label>Y-Axis</mat-label>
+          <mat-select [(ngModel)]="yAxis" (ngModelChange)="createChart()">
+            @for(axis of availableAxes; track axis) {
+              <mat-option [value]="axis">{{ axis }}</mat-option>
+            }
+          </mat-select>
+        </mat-form-field>
+      </div>
+
+      @if (data().length) {
+        <div class="chart-container" style="position: relative; height:40vh; width:80vw">
+          <canvas #chartCanvas></canvas>
+        </div>
+      } @else {
+        <div class="text-center py-12">
+          <mat-icon class="text-6xl text-gray-300 mb-4">info</mat-icon>
+          <p class="text-gray-500">No data available to display.</p>
+        </div>
+      }
+
+      @if (selectedPoint(); as point) {
+        <div class="mt-4 p-4 bg-gray-100 rounded">
+          <h2 class="text-lg font-bold">Selected Point</h2>
+          <p>X: {{ point.x }}</p>
+          <p>Y: {{ point.y }}</p>
+          <p>Temp: {{ point.temp }}</p>
+        </div>
+      }
+    </div>
+  `
+})
+export class DataVisualizationComponent implements AfterViewInit {
+  @ViewChild('chartCanvas') chartCanvas: ElementRef<HTMLCanvasElement> | undefined;
+  chart: Chart | undefined;
+
+  data = signal<DataPoint[]>([
+    { x: 1, y: 2, temp: 25 },
+    { x: 2, y: 4, temp: 30 },
+    { x: 3, y: 8, temp: 35 },
+    { x: 4, y: 16, temp: 40 },
+  ]);
+
+  selectedPoint = signal<DataPoint | null>(null);
+
+  availableAxes = ['x', 'y', 'temp'];
+  xAxis = 'x';
+  yAxis = 'y';
+
+  ngAfterViewInit() {
+    this.createChart();
+  }
+
+  createChart() {
+    if (this.chart) {
+      this.chart.destroy();
+    }
+
+    if (!this.chartCanvas || !this.data().length) {
+      return;
+    }
+
+    const context = this.chartCanvas.nativeElement.getContext('2d');
+    if (context) {
+      this.chart = new Chart(context, {
+        type: 'scatter',
+        data: {
+          datasets: [{
+            label: 'Sample Data',
+            data: this.data().map(d => ({ x: d[this.xAxis], y: d[this.yAxis] })),
+            backgroundColor: 'rgba(75, 192, 192, 0.6)',
+          }]
+        },
+        options: {
+          responsive: true,
+          onClick: (event, elements) => {
+            if (elements.length > 0) {
+              const element = elements[0];
+              const dataPoint = this.data()[element.index];
+              this.selectedPoint.set(dataPoint);
+            }
+          },
+          scales: {
+            x: {
+              type: 'linear',
+              position: 'bottom',
+              title: {
+                display: true,
+                text: this.xAxis
+              }
+            },
+            y: {
+              title: {
+                display: true,
+                text: this.yAxis
+              }
+            }
+          },
+          plugins: {
+            tooltip: {
+              callbacks: {
+                label: (context) => {
+                  const point = context.raw as DataPoint;
+                  return `${this.xAxis}: ${point.x}, ${this.yAxis}: ${point.y}`;
+                }
+              }
+            }
+          }
+        }
+      });
+    }
+  }
+
+  exportChart() {
+    if (this.chart) {
+      const a = document.createElement('a');
+      a.href = this.chart.toBase64Image();
+      a.download = 'chart.png';
+      a.click();
+    }
+  }
+}
